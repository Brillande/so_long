			if (map_array[0][j] != '1' || map_array[rows_of_array -1][j]  != '1')
			{
		//		ft_printf("valoo %d - %d", map_array[0][j] - '0',  map_array[i][0] -  '0');
				perror("error no esta rodeado de muros");
//				return(0);
			}
			if (map_array[x][0]  != '1' || map_array[cols_of_array -1][0]   != '1' )
			{
				perror("error no esta rodeado de muros");
//				return(0);
			}


3 X 7			
